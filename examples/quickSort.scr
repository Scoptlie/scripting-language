
qsort = func(arr, lo, hi) {
	if lo >= hi || lo < 0 {
		return
	}
	
	var pivot = arr[hi]
	var p = lo
	var i = lo, while i <= hi-1 {
		if arr[i] <= pivot {
			var t = arr[p]
			arr[p] = arr[i]
			arr[i] = t
			p = p + 1
		}
		i = i + 1
	}
	
	var t = arr[p]
	arr[p] = arr[hi]
	arr[hi] = t
	
	qsort(arr, lo, p-1)
	qsort(arr, hi, p+1)
}

arr = [4, 2, 5, 1, 3]
printArr = func() {
	print(arr[0] + ", " + arr[1] + ", " + arr[2] + ", " + arr[3] + ", " + arr[4])
}

printArr()
qsort(arr, 0, 4)
printArr()
